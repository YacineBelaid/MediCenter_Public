generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:3306/Write_DMC"
}

model Patient {
  id             Int  @id @default(autoincrement()) @db.UnsignedInt
  nom            Int?
  prenom         Int?
  date_naissance Int?
}
